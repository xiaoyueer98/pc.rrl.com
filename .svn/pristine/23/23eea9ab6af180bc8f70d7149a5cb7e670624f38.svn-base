.return-index{
	position: fixed;
	width: 42px;
	height: 42px;
	right:82px;
	bottom: 100px;
	background: #b5b5b5;
}
.scrol-top{
	right:30px;
}
.return-index span{
	display: block;
	width: 100%;
	height: 100%;
	background: url(../img/icon5.png) no-repeat center;
	position: absolute;
	top: 0;
	left: 0;
}
.scrol-top span{
	background: url(../img/icon6.png) no-repeat center;
}
.return-index i{
	display: block;
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	background: #000;
	filter:alpha(opacity=0); /*IE滤镜，透明度50%*/
	-moz-opacity:0; /*Firefox私有，透明度50%*/
	opacity:0;/*其他，透明度50%*/
}
.return-index a{
	color:#fff;
	width: 24px;
	margin-top: 5px;
	margin-left: 10px;
	font-size: 12px;
	display: block;
	position: absolute;
	display: none;
}
