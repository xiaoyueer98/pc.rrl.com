    <script type="text/javascript">
        $(document).ready(function() {
            $("#btn").click(function() {
                $.post(
                        "/Home/Upresume/is_login",
                        "",
                        function(re) {
                            if ($.trim(re) == 1) {
                                location.href = "/Home/Login/Recommended/act/addResume.html";
                                return;
                            } else if ($.trim(re) == 2) {
                                $(".mask").show();
                                $("#like_alert_kuang").show();
                                return;
                            } else {
                                $(".mask").show();
                                $(".denglu").show();
                                return;
                            }
                        }, "json");
            })
            $("#alert_ok").click(function() {
                $(".mask").hide();
                $("#like_alert_kuang").hide();
            })
        })
    </script>
