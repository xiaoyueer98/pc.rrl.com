</div>
    <script>
        function showDisMsg(msg) {
            sys_window(msg);
        }
        $(".m2-l>div").click(function() {
            $(this).addClass("m");
            $(this).find("h6").addClass("m");
            $(this).find("p").addClass("m");
            $(this).siblings().removeClass("m");
            $(this).siblings().find("h6").removeClass("m");
            $(this).siblings().find("p").removeClass("m");
        });
        $(".position-name .img-btn").click(function(){
            if($(this).hasClass("current")){
                $(this).parent().parent().parent().next().next().hide().siblings(".state2").hide();
                $(this).removeClass("current")
            }else{
                $(this).parent().parent().parent().next().next().show().siblings(".state2").hide();
                $(this).addClass("current")
            };
        })
        $(".track-state table tr .shenhe").click(function(){
			if($(this).hasClass("current")){
				$(this).parent().parent().next().hide().siblings(".state").hide();
				$(this).removeClass("current")
			}else{
				$(this).parent().parent().next().show().siblings(".state").hide();
				$(this).addClass("current")
			};
		});

        /* 当鼠标移到表格上是，当前一行背景变色 */
        $(document).ready(function(){
            $(".data_list tr td").mouseover(function(){
                $(this).parent().find("td").css("background-color","#dad7d7");
            });
        })
      /* 当鼠标在表格上移动时，离开的那一行背景恢复 */
      $(document).ready(function(){
            $(".data_list tr td").mouseout(function(){
                $(this).parent().find("td").css("background-color","#f0f0f0");
            });
      })
      
      $(document).ready(function(){
            var color="#f0f0f0"
            $(".data_list tr:odd td").css("background-color",color);  //改变偶数行背景色
            /* 把背景色保存到属性中 */
            $(".data_list tr:odd").attr("bg",color);
            $(".data_list tr:even").attr("bg","#fff");
      })
    </script>
</body>

</html>