.pc-zhuce dl .yanzheng b{float:left;display:block;margin-right:10px;width:18px;height:18px;border:1px solid #ccc;border-radius:5px;background:url(../img/duihao.png) center no-repeat}
.pc-zhuce dl .yanzheng span{float:left;display:block;height:18px;line-height:18px}
.pc-zhuce dl .yanzheng .radio1{float:left;margin-top:4px;margin-right:10px;display:block}
.pc-zhuce dl .yanzheng span a{color:#2380b8}
.pc-zhuce dl button{position:absolute;top:370px;display:block;margin-left:30px;width:294px;height:40px;outline:0;border:0;border-radius:8px;background:#2380b8;color:#fff;cursor:pointer;font-weight:700;font-size:14px}
.mask,.reward{display:none}
.pc-zhuce dl dd{float:right;width:240px;position:relative;z-index:3;background:#fff;height:361px;top:-28px;left:-7px}
.mask,.mask1{position:fixed;z-index:15!important;width:100%;height:100%;background:#000;opacity:.7;filter:alpha(opacity=70)}
.pc-zhuce dl dd .yd{margin-top:75px;text-align:center}
.pc-zhuce dl dd .yd a{color:#2284bb;font-size:18px}
.pc-zhuce dl dd .yd span{color:#bcbcbc;font-size:18px}
.pc-zhuce dl dd .yd em{color:#2284bb;font-size:18px;cursor:pointer}
.pc-zhuce dl dd p{margin-top:40px;color:#ccc;text-align:center}
.pc-zhuce dl dd p a{color:#2284bb;text-align:center;font-size:14px}
.pc-zhuce dl dd ul{padding-left:10px}
.pc-zhuce dl dd ul li{float:left;padding:17px 5px 0 0}
.mask1{display:block}
.denglu ul.ul_ul1{margin-left:195px;margin-top:55px}
.denglu ul li.li_1{width:50px;float:left;margin-left:10px}
#wxts_text{padding:10px 0 10px 30px;width:100%;border:1px dashed #c6c6c6;border-radius:2px}
#titl_ts{position:relative;top:11px;left:20px;padding:0 10px;background:#FFF;width:80px;color:green;height:30px;text-align:center;line-height:30px;font-size:15px!important}
