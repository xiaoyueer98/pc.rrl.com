/* -------------label-------------- */
.tb-list{
	width: 685px;
	margin-top: 20px;
	position: relative;
	padding-bottom: 50px;
}
.tb-list ul.th-head{
	height: 48px;
	width: 100%;
	background: #0983c6;
}
.tb-list ul.th-head li{
	height: 48px;
	line-height: 48px;
	color: #ffffff;
	font-size: 14px;
	text-align: center;
	border-left:1px #0983c6 solid;
}
.tb-list .bt-odiv{
	height: 47px;
	border:1px #b3b3b3 solid;
	margin-top: 4px;
	position: relative;
}
.tb-list .bt-odiv div.zt{
	background: url(/Public/img/zt-pic.png) no-repeat center;
}
.tb-list .bt-odiv div.zt .zt-text{
	width: 460px;
	position: absolute;
	left: 120px;
	z-index: 1000;
	display: none;
}
.tb-list .bt-odiv div.zt .zt-text i{
	background: #ffd2d2;
	display: block;
	float: left;
	padding:10px;
	border: 1px #979797 solid;
	text-align: left;
	line-height: 18px;
	font-size: 14px;
}
.tb-list .bt-odiv div.zt .zt-text i a{
	color: #e99d41;
	font-size: 14px;
	font-weight: bold;
}
.tb-list .bt-odiv div.zt .zt-text b{
	display: block;
	width: 12px;
	height: 9px;
	background: url(/Public/img/icon12.png) no-repeat;
	position: absolute;
	bottom: -8px;
	left:40px;
}
.tb-list .bt-odiv div{
	height: 47px;
	line-height: 47px;
	text-align: center;
	color: #5c5c5c;
	border-right:1px #b3b3b3 solid;
}
.tb-list .bt-odiv div.last-sp{
	border: none;
}
.tb-list .bt-odiv div.last-sp a{
	display: block;
	width: 30px;
	margin-top:5px;
	padding-top: 7px;
	float: left;
	margin-left: 20px;
	color: #0983c6
}
.tb-list .bt-odiv div.last-sp a.edit{
	background: url(../img/rcmd-img/edit-icon.png) no-repeat center top;
}
.tb-list .bt-odiv div.last-sp a.delete{
	background: url(../img/rcmd-img/delete-icon.png) no-repeat center top;
}
.tb-list .inner{width:665px;height:28px;background:#e5e7e6;padding-top:13px;position:absolute;bottom:0;padding-right:20px}
.tb-list .inner li{height:18px;line-height:18px;text-align:center;background:#fff;width:18px;border:1px #aaaaaa solid;font-size:12px;margin:0 5px;color:#777777;cursor:pointer}
.tb-list .inner li.active{background:#2e6ab1;color:#fff}
.tb-list .inner li a{display:block;width:100%;height:100%;color:#333333}
.tb-list .inner li a.active{color:#fff}
.tb-list .inner .total{color:#333333;text-align:center;line-height:18px;margin:0 10px}
.tb-list .inner .form .text{line-height:18px}
.tb-list .inner .form .input{width:24px;border:1px #aaaaaa solid;margin:0 10px}
.tb-list .inner .form .btn{width:36px;cursor:pointer;margin-left:10px;line-height:18px;text-align:center;color:#0983c6;background:#d0edfd;border:1px #3ca3dd solid}