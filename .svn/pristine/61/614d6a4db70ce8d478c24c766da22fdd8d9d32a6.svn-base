html,body {
	margin: 0;
	padding: 0;
	height: 100%;
}
.clearfix:after {
	clear: both;
	display: block;
	visibility: hidden;
	height: 0;
	content: " ";
	font-size: 0;
}
body {
	background-image: url("../img/bg.jpg");
	background-position: center center;
	background-size: cover;
	background-repeat: no-repeat;
	background-attachment: fixed;
	font-weight: 300;
	font-size: 12px;
	font-family: "Source Sans Pro",sans-serif;
}
* {
	outline: medium;
	font-family: '微软雅黑';
}
.cat .sousuo {
	width: 200px;
	height: 34px;
	border-top: 1px #f6f5f4 solid;
	border-bottom: 1px #f6f5f4 solid;
}
.cat .sousuo .myUl {
	margin: 0;
	padding: 0;
	width: 100%;
	height: 100%;
	list-style: none;
}
.cat .sousuo .myUl li {
	position: relative;
	width: 100%;
	height: 100%;
}
.undis {
	display: none;
}
.dis {
	display: block;
}
.cat .sousuo .myUl input {
	position: absolute;
	top: 7px;
	left: 29px;
	display: none;
	padding-left: 10px;
	width: 120px;
	height: 18px;
	outline: medium;
	border: 0;
	border: 1px #ccc solid;
	border-radius: 10px 0 0 10px;
	font-size: 12px;
}
.cat .sousuo .myUl li .mysp {
