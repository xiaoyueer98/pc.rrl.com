            $(".mask").hide();
            $(".secord").show();
            $("#file_upload-queue").html("");
            if (title) {
                $("#jianliurl").html('<a href=\'' + updatePath + updateFile + '\'>' + title + "--" + updateFile + '</a>');
            } else {
                $("#jianliurl").html('<a href=\'' + updatePath + updateFile + '\'>' + updateFile + '</a>');
            }

        }

        //实例化编辑器
        //建议使用工厂方法getEditor创建和引用编辑器实例，如果在某个闭包下引用该编辑器，直接调用UE.getEditor('editor')就能拿到相关的实例
