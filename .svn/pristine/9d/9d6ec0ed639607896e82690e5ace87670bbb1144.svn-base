    <script type="text/javascript">
        function savaResumeType(type) {
            var username = $("#username" + type).val();
            var age = $("#age" + type).val();
            var state = $("#state" + type).val();
            var mobile = $("#mobile" + type).val();
            var email = $("#emailed" + type).val();
            var qqnum = $("#qqnum" + type).val();
            var match_company = $("#match_company" + type).val();
            var match_educational = $("#match_educational" + type).val();
            var match_treatment = $("#match_treatment" + type).val();
            var match_area = $("#match_area" + type).val();
            var updateFile = $("#updateFile").val();
          
            //alert(state)

            if (!username) {
                $("#errorClass" + type).html('请填写姓名');
                $("#errorClass" + type).show();
                return false;
            }
            if (!age) {
                $("#errorClass" + type).html('请填写年龄');
                $("#errorClass" + type).show();
                return false;
            }
            if (!state) {
                $("#errorClass" + type).html('请选择在职状态');
                $("#errorClass" + type).show();
                return false;
            }
            if (state == '请选择') {
                $("#errorClass" + type).html('请填写候选人在职状态');
                $("#errorClass" + type).show();
                return false;
            }
            if (!mobile) {
                $("#errorClass" + type).html('请填写手机');
                $("#errorClass" + type).show();
                return false;
            }
            if (!email) {
                $("#errorClass" + type).html('请填写邮箱');
                $("#errorClass" + type).show();
                return false;
            }
            if (match_company == "请选择") {
                $("#errorClass" + type).html('请选择公司背景');
                $("#errorClass" + type).show();
                return false;
            }
            if (match_educational == "请选择") {
                $("#errorClass" + type).html('请选择学历');
                $("#errorClass" + type).show();
                return false;
            }
            if (match_treatment == "请选择") {
                $("#errorClass" + type).html('请选择期望薪资');
                $("#errorClass" + type).show();
                return false;
            }
            if (!match_area) {
                $("#errorClass" + type).html('请填写被推荐人居住地');
                $("#errorClass" + type).show();
                return false;
            }
            //////////////////////////////委托系统管理
      
            //关键字
            var keyword = $("#keyword"+type).val();
            //细分行业
            var match_industry = $("#match_industry"+type).val();
            //候选人职位级别
            var match_title = $("#match_title"+type).val();
            //熟练程度
            var match_skill = $("#match_skill"+type).val();
            if(!keyword){
                $("#errorClass" + type).html('请填写投递关键词');
                $("#errorClass" + type).show();
                return false;
            }
            if(!match_industry || match_industry == '请选择'){
                $("#errorClass" + type).html('请选择细分行业');
                $("#errorClass" + type).show();
                return false;
            }
             if(!match_title || match_title == '请选择'){
                $("#errorClass" + type).html('请选择职位级别');
                $("#errorClass" + type).show();
                return false;
            }
            if(!match_skill || match_skill == '请选择'){
                $("#errorClass" + type).html('请选择核心技术的熟练情况');
                $("#errorClass" + type).show();
                return false;
            }
            if (type == 1) {
                var because = ue1.getContentTxt();
                if (!because) {
                    $("#errorClass" + type).html('推荐理由不能为空');
                    $("#errorClass" + type).show();
                    return false;
                }
                if (!updateFile) {
                    $("#errorClass" + type).html('请上传附件简历');
                    $("#errorClass" + type).show();
                    return false;
                }
            } else {
                /*
                 //自我评价
                 var because2 = ue2.getContentTxt();
                 if (!because2) {
                 $("#errorClass" + type).html('自我评价不能为空');
                 $("#errorClass" + type).show();
                 return false;
                 }
                 */
                //教育经历
                var because3 = ue3.getContentTxt();
                if (!because3) {
                    $("#errorClass" + type).html('教育经历不能为空');
                    $("#errorClass" + type).show();
                    return false;
                }
                //工作经历
                var because4 = ue4.getContentTxt();
                if (!because4) {
                    $("#errorClass" + type).html('工作经历不能为空');
                    $("#errorClass" + type).show();
                    return false;
                }

                //推荐理由
                var because6 = ue6.getContentTxt();
                if (!because6) {
                    $("#errorClass" + type).html('推荐理由不能为空');
                    $("#errorClass" + type).show();
                    return false;
                }

            }
            $.post("__URL__/isNewUser.html", {}, function(data) {
                if (data == 2) {
                    $("input[name='jltype']").val(type);
                    $(".mask").show();
                    $(".hxrmssj").show();
                } else {
                    $(".resubmitForm" + type).submit();
                }
            }, "json")
            return false;
        }

        $(".zaigoutongyixia").click(function() {
            var type = $("input[name='jltype']").val();
            $(".resubmitForm" + type).submit();
        });
        $(".querentuijian").click(function() {
            var audstarttime = $("#audstarttime").val();
            if (!audstarttime) {
                alert("请输入面试时间！");
                return false;
            }
            var type = $("input[name='jltype']").val();
            $("input[name='mssj']").val(audstarttime);
            $(".resubmitForm" + type).submit();


        });

    </script>
