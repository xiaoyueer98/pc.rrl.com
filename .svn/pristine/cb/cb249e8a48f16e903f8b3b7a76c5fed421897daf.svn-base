.clearfix:after {
    clear: both;
    display: block;
    visibility: hidden;
    height: 0;
    content: " ";
    font-size: 0;
}
.wx{
    width: 100%;
    height: 25px;
    line-height: 25px;
    background: #ffd2d2;
    text-align: center;
    color: #333333;
    position: fixed;
}
.wx i{
    display: block;
    float: right;
    width: 25px;
    height: 25px;
    background: url(/Public/img/ss.png) no-repeat center;
    background-size: 50% 50%;
}
.thelist ul{
    width: 100%;
    padding-top:10px;
}
.thelist ul h3{
    margin-left:10px;
    padding-left: 10px;
    border-left: 3px #2380b8 solid;
    font-size: 16px;
    height: 23px;
    line-height: 23px;
    margin-bottom: 10px;
}
.thelist ul li{
    width: 100%;
    height: 30px;
    border-bottom:1px #cfcfcf solid;
    height: 34px;
    line-height: 30px;
    font-size: 12px;
}
.thelist ul li span{margin-left: 7px;margin-right: 7px;
                    display: inline-block;
                    float: left;}

.thelist ul li span{margin-right: 7px;margin-right: 7px;
                    display: inline-block;
                    float: left;}
.jl ul li span{
    margin-right: 7px;
    display: inline-block;
    float: left;
    width: 65px;
}
.jl ul li em{
    display: block;
    float: left;
    width: 68%;
    height: 25px;
    line-height: 25px;
    margin-top:8px;
}
.jl ul li a{
    display: block;
    float: left;
    width: 70%;
    height: 25px;
    line-height: 25px;
    margin-top:5px;
}
.jl ul li input{
    display: block;
    float: left;
    width: 70% !important;
    height: 25px!important;
    line-height: 25px!important;
    margin-top:3px!important;
    display: none;
}
.thelist ul li span.sp{
    margin-right: 7px;
    display: inline-block;
    float: left;
}
.thelist ul li em{
    margin-right: 20px;
    display: inline-block;
    height: 20px;
    float: left;
}
.thelist ul li label.nan{
    width: 15px;
    height: 15px;
    background: #206daf;
    border-radius: 90px;
    display: inline-block;
    float: left;
    margin:0 5px;
    margin-top:8px;

}
.thelist ul li label.m{
    width: 13px;
    height: 13px;
    background: #ffffff;
    border:1px #ccc solid;
}
.thelist ul li input{
    width: 140px;
    float: left;
    border: 1px #ccc solid;
    padding-left: 10px;
    margin-top:6px;
}
.thelist ul li .modify{
    width: 98%;
    display: block;
    margin: 0 auto;
    height: 30px;
    color: #ffffff;
    background: #206daf;
    border: none;
    font-size: 14px;
    margin-bottom: 10px;
    border-radius: 5px;
}

.about-us{
    width: 96%;
    margin:0 auto;
}

.about-us div.div-img{
    width: 100%;
    margin-top: 10px;
}
.about-us div img{
    width: 100%;
}
.about-us p{
    line-height: 22px;
    font-size: 14px;
    color: #334558;
    text-indent: 24px;
}
.about-us p b{
    color: #334558
}