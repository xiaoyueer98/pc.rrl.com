}
.aligncenter{
    -webkit-box-align:center;
    -moz-box-align:center;
    -ms-box-align:center;
    -o-box-align:center;
}
.alignright{
    -webkit-box-align:right;
    -moz-box-align:right;
    -ms-box-align:right;
    -o-box-align:right;
}
.packcenter{
    -webkit-box-pack:center;
    -moz-box-pack:center;
    -ms-box-pack:center;
